{
	"compilerOptions": {
		"target": "ESNext",
		"lib": [
			"ESNext"
		],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"types": [
			"@cloudflare/workers-types"
		],
		"resolveJsonModule": true,
		"noEmit": true,
		"isolatedModules": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"skipLibCheck": true
	}
}